{
    "extends": ["tslint-config-airbnb"],
    "rules": {
      "import-name": false,
      "max-line-length": [true, 300],
      "ter-indent": [
        true,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "trailing-comma": false,
      "prefer-template": false
    },
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts",
        "coverage/lcov-report/*.js"
      ]
    }
  }
  